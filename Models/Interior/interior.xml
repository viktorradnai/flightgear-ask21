<?xml version="1.0"?>

<PropertyList>

 <path>interior.ac</path> 

    <!--COCKPIT-->
    <!-- Yaw string -->
    <model>
        <name>yawstring</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/wool-thread/wool-thread.xml</path>
        <offsets>
            <x-m>-3.50</x-m>
            <y-m> 0</y-m>
            <z-m>0.90</z-m>
            <pitch-deg>0</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>panelF</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelF.xml</path>
    </model>
    <model>
        <name>panelR</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelR.xml</path>
    </model>
    <!--Pilots-->
    <model>
        <name>pilots</name>
        <path>Aircraft/ASK21/Models/Interior/Pilots/pilots.xml</path>
    </model>
    <!--Small windows-->
    <animation>
        <type>slider</type>
        <object-name>sliding_window.front</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/frontwin/position-norm</property>
                <factor>-0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.front</object-name>
        <property>sim/model/door-positions/frontwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.18</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-2.94858</x1-m>
            <y1-m>-0.31449</y1-m>
            <z1-m>0.70963</z1-m>
            <x2-m>-2.63997</x2-m>
            <y2-m>-0.32957</y2-m>
            <z2-m>0.76366</z2-m>
        </axis>
    </animation>
    <animation>
        <type>slider</type>
        <object-name>sliding_window.back</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>sim/model/door-positions/rearwin/position-norm</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.01</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.01</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/rearwin/position-norm</property>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.1</ind>
                <dep>0.0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>0.12</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-1.80509</x1-m>
            <y1-m>-0.31714</y1-m>
            <z1-m>0.92524</z1-m>
            <x2-m>-2.0615</x2-m>
            <y2-m>-0.31714</y2-m>
            <z2-m>0.90689</z2-m>
        </axis>
    </animation>
    <!--airbrake grip-->
    <animation>
        <type>slider</type>
        <object-name>grip.airbrake</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>property-adjust</command>
                <property>/controls/engines/engine/throttle</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.25</dep>
            </entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>-1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>-0.025</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>grip.airbrake</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.045</dep>
            </entry>
        </interpolation>
        <axis>
            <x>0</x>
            <y>0</y>
            <z>1</z>
        </axis>
    </animation>
    <!--stick front-->
    <animation>
        <type>rotate</type>
        <object-name>stick-front</object-name>
        <property>/controls/flight/elevator</property>
        <factor>10</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>-3.17203</x-m>
            <y-m>0.0</y-m>
            <z-m>0.32481</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>stick-front</object-name>
        <property>/controls/flight/aileron</property>
        <factor>10</factor>
        <offset>0</offset>
        <center>
            <x-m>-3.17203</x-m>
            <y-m>0.0</y-m>
            <z-m>0.32481</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <!--stick rear-->
    <animation>
        <type>rotate</type>
        <object-name>stick-rear</object-name>
        <property>/controls/flight/elevator</property>
        <factor>10</factor>
        <offset-deg>0</offset-deg>
        <center>
            <x-m>-1.99529</x-m>
            <y-m>0.0</y-m>
            <z-m>0.38177</z-m>
        </center>
        <axis>
            <x>0</x>
            <y>-1</y>
            <z>0</z>
        </axis>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>stick-rear</object-name>
        <property>/controls/flight/aileron</property>
        <factor>10</factor>
        <offset>0</offset>
        <center>
            <x-m>-1.99529</x-m>
            <y-m>0.0</y-m>
            <z-m>0.38177</z-m>
        </center>
        <axis>
            <x>-1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
    <!--Cockpit Shader-->
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/interior-shadow</inherits-from>
        <object-name>interiorF</object-name>
        <object-name>seatF</object-name>
    </effect>
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/interior-shadow-back</inherits-from>
        <object-name>interiorR</object-name>
        <object-name>seatR</object-name>
    </effect>
    <!--CANOPY-->
    <animation>
        <type>rotate</type>
        <object-name>canopy.front.left</object-name>
        <object-name>canopy.front.right</object-name>
        <object-name>canopy.front.int</object-name>
        <object-name>panelF</object-name>
        <object-name>yawstring</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-50</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
        <center>
            <x-m>-3.56113</x-m>
            <z-m>0.82</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.front.left</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>-60</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-60</dep>
            </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
        <center>
            <x-m>-3.04127</x-m>
            <y-m>-0.2964</y-m>
            <z-m>0.65308</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.front.right</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>60</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>60</dep>
            </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
        <center>
            <x-m>-3.04127</x-m>
            <y-m>0.2964</y-m>
            <z-m>0.65308</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.back.left</object-name>
        <object-name>canopy.back.right</object-name>
        <object-name>canopy.back.int</object-name>
        <object-name>sliding_window.back</object-name>
        <property>sim/model/door-positions/canopyB/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>50</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
        <center>
            <x-m>-1.26909</x-m>
            <z-m>1.11</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.back.left</object-name>
        <property>sim/model/door-positions/canopyB/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>-60</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-60</dep>
            </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
        <center>
            <x-m>-1.94174</x-m>
            <y-m>-0.33063</y-m>
            <z-m>0.77602</z-m>
        </center>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.back.right</object-name>
        <property>sim/model/door-positions/canopyB/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>60</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>60</dep>
            </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
        <center>
            <x-m>-1.94174</x-m>
            <y-m>0.33063</y-m>
            <z-m>0.77602</z-m>
        </center>
    </animation>
    
    <!--SHADERS/EFFECTS-->
    <!--Enable/disable windows for better visibility-->
    <animation>
        <type>select</type>
        <condition>
            <property>/sim/model/window</property>
        </condition>
        <object-name>canopy.front.int</object-name>
        <object-name>canopy.back.int</object-name>
        <object-name>sliding_window.front</object-name>
        <object-name>sliding_window.back</object-name>
    </animation>
    <effect>
        <inherits-from>Models/Effects/interior-glass-front</inherits-from>
        <object-name>canopy.front.int</object-name>
        <object-name>sliding_window.front</object-name>
    </effect>
    <effect>
        <inherits-from>Models/Effects/interior-glass-back</inherits-from>
        <object-name>canopy.back.int</object-name>
        <object-name>sliding_window.back</object-name>
    </effect>
</PropertyList>