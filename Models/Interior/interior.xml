			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<condition>
					<equals>
						<property>/sim/model/variant</property>
						<value>0</value>
					</equals>
				</condition>
				<command>property-adjust</command>
				<property>/controls/engines/engine/throttle</property>
				<factor>0.05</factor>
				<min>0</min>
				<max>1</max>
			</binding>
			<binding>
				<condition>
					<equals>
						<property>/sim/model/variant</property>
						<value>1</value>
					</equals>
				</condition>
				<command>property-adjust</command>
