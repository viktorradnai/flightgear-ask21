        <name>panelF</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelF.xml</path>
    </model>
    <model>
        <name>panelFmi</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelF-mi.xml</path>
    </model>
    <model>
        <name>panelR</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/panelR.xml</path>
    </model>
    <model>
        <name>panelRmi</name>
	<path>Aircraft/ASK21/Models/Interior/Panel/panelR-mi.xml</path> <!--to come-->
    </model>
    <!--Engine Console, only present in -mi-->
    <model>
        <name>engconsole</name>
        <path>Aircraft/ASK21/Models/Interior/engconsole.xml</path>
    </model>
    
	<animation>
		<type>select</type>
		<object-name>engconsole</object-name> 
		<object-name>panelFmi</object-name>   
		<object-name>panelRmi</object-name>   
		<condition>
			<equals>
				<property>sim/multiplay/generic/int[14]</property>
				<value>1</value>
			</equals>
		</condition>
	</animation>
	<animation>
		<type>select</type>
		<object-name>panelF</object-name>   
		<object-name>panelR</object-name>  
		<condition>
			<equals>
				<property>sim/multiplay/generic/int[14]</property>
				<value>0</value>
			</equals>
		</condition>
	</animation>
    
    <!--Pilots-->
    <model>
        <name>pilots</name>
        <path>Aircraft/ASK21/Models/Interior/Pilots/pilots.xml</path>
    </model>
    <!--Airbrake Handle-->
    <animation>
        <type>slider</type>
        <object-name>airbrake-front</object-name>
        <object-name>airbrake-rear</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
		<condition>
			<equals>
				<property>/sim/model/variant</property>
				<value>0</value>
			</equals>
		</condition>
                <command>property-adjust</command>
                <property>/controls/engines/engine/throttle</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
            <binding>
		<condition>
			<equals>
				<property>/sim/model/variant</property>
				<value>1</value>
			</equals>
		</condition>
                <command>property-adjust</command>
                <property>/controls/flight/spoilers</property>
                <factor>0.05</factor>
                <min>0</min>
                <max>1</max>
            </binding>
        </action>
        <hovered>
            <binding>
		<condition>
			<equals>
				<property>/sim/model/variant</property>
				<value>0</value>
			</equals>
		</condition>
                <command>set-tooltip</command>
                <tooltip-id>airbrake</tooltip-id>
                <label>%s</label>
                <mapping>nasal</mapping>
                <property>/controls/engines/engine/throttle</property>
                <script>
                    return sprintf("Speedbrake %d%% out", 100-arg[0]*100);
                </script>
            </binding>
            <binding>
		<condition>
			<equals>
				<property>/sim/model/variant</property>
				<value>1</value>
			</equals>
		</condition>
                <command>set-tooltip</command>
                <tooltip-id>airbrake-mi</tooltip-id>
                <label>%s</label>
                <mapping>nasal</mapping>
                <property>/controls/flight/spoilers</property>
                <script>
                    return sprintf("Speedbrake %d%% out", 100-arg[0]*100);
                </script>
            </binding>
        </hovered>
    </animation>
    <animation>
	<condition>
		<equals>
			<property>/sim/model/variant</property>
			<value>0</value>
		</equals>
	</condition>
        <type>translate</type>
        <object-name>airbrake-front</object-name>
        <property>/controls/engines/engine/throttle</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>1</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0</ind>
                <dep>.21</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.10624</x1-m>
            <y1-m>-0.24893</y1-m>
            <z1-m>0.47521</z1-m>
            <x2-m>-2.81179</x2-m>
            <y2-m>-0.27476</y2-m>
            <z2-m>0.53454</z2-m>
        </axis>
    </animation>
    <animation>
	<condition>
		<equals>
			<property>/sim/model/variant</property>
			<value>1</value>
		</equals>
	</condition>
        <type>translate</type>
        <object-name>airbrake-front</object-name>
        <property>/controls/flight/spoilers</property>
        <factor>1</factor>
        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>.21</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-3.10624</x1-m>
            <y1-m>-0.24893</y1-m>
            <z1-m>0.47521</z1-m>
            <x2-m>-2.81179</x2-m>
            <y2-m>-0.27476</y2-m>
            <z2-m>0.53454</z2-m>
        </axis>
    </animation>
