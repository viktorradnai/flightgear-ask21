		<equals>
			<property>/sim/model/variant</property>
			<value>0</value>
		</equals>
	</condition>
        <type>translate</type>
        <object-name>airbrake-rear</object-name>
        <property>/controls/engines/engine/throttle</property>
        <interpolation>
