<?xml version="1.0"?>
<PropertyList>
    <path>interior.ac</path>
    <!--COCKPIT-->
    <!-- Yaw string -->
    <model>
        <name>yawstring</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/wool-thread/wool-thread.xml</path>
        <offsets>
            <x-m>-3.43276</x-m>
            <y-m> 0</y-m>
            <z-m>0.88</z-m>
            <pitch-deg>22</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>yawstring2</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/wool-thread/wool-thread.xml</path>
        <offsets>
            <x-m>-2.22</x-m>
            <y-m> 0</y-m>
            <z-m>1.18</z-m>
            <pitch-deg>40</pitch-deg>
        </offsets>
    </model>
