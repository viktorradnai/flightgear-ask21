        <interpolation>
            <entry>
                <ind>0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>.21</dep>
            </entry>
        </interpolation>
        <axis>
            <x1-m>-2.17081</x1-m>
