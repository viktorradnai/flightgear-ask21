<?xml version="1.0"?>

<PropertyList>

 <path>panelR.ac</path>

    <!--COCKPIT-->
    <!--FLARM-->
    <model>
        <name>flarm</name>
        <path>Instruments/FLARM/FLARM.xml</path>
        <offsets>
            <x-m>-2.2</x-m>
            <y-m>0.0</y-m>
            <z-m>0.89</z-m>
            <pitch-deg>-10</pitch-deg>
        </offsets>
    </model>
    <!--REAR INSTRUMENTS-->
    <!--Altimeter-->
    <model>
        <name>alt</name>
        <path>Instruments/alti-4110/winter-4110.xml</path>
        <offsets>
            <x-m>-2.18746</x-m>
            <y-m>-0.06</y-m>
            <z-m>0.73</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>alt-1</name>
        <path>Instruments/alt/Altimeter.xml</path>
        <offsets>
            <x-m>-2.18746</x-m>
            <y-m>-0.06</y-m>
            <z-m>0.73</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <animation>
        <type>select</type>
        <object-name>alt</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/alt</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>alt-1</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/alt</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    <!--ASI-->
    <model>
        <name>asi2</name>
        <path>Instruments/winter-asi-ask21-kph/80mm_asi_6fms4.xml</path>
        <offsets>
            <x-m>-2.193</x-m>
            <y-m>-0.11</y-m>
            <z-m>0.819</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>asi2-1</name>
        <path>Instruments/winter-asi-6221/winter-asi-6221.xml</path>
        <offsets>
            <x-m>-2.193</x-m>
            <y-m>-0.11</y-m>
            <z-m>0.819</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>asi2-2</name>
        <path>Instruments/winter-asi-ask21-knots/80mm_asi_6fms4.xml</path>
        <offsets>
            <x-m>-2.193</x-m>
            <y-m>-0.11</y-m>
            <z-m>0.819</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <animation>
        <type>select</type>
        <object-name>asi2</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/asi</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>asi2-1</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/asi</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>asi2-2</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/asi</property>
                <value>2</value>
            </equals>
        </condition>
    </animation>
    <!--Variometer-->
    <model>
        <name>vario</name>
        <path>Instruments/vario-5stv5/winter-vario-5stv5.xml</path>
        <offsets>
            <x-m>-2.19714</x-m>
            <y-m>0.11</y-m>
            <z-m>0.819</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>vario-1</name>
        <path>Instruments/vario-knots/winter-vario-5stv5-knots.xml</path>
        <offsets>
            <x-m>-2.19714</x-m>
            <y-m>0.11</y-m>
            <z-m>0.819</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <animation>
        <type>select</type>
        <object-name>vario</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/vsi</property>
                <value>0</value>
            </equals>
        </condition>
    </animation>
    <animation>
        <type>select</type>
        <object-name>vario-1</object-name>
        <condition>
            <equals>
                <property>/sim/model/livery/instruments/vsi</property>
                <value>1</value>
            </equals>
        </condition>
    </animation>
    <!--Compass-->
    <model>
        <name>compass2</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/57mm_compass/57mm_compass.xml</path>
        <offsets>
            <x-m>-2.185</x-m>
            <y-m>-0.205</y-m>
            <z-m>0.748</z-m>
            <pitch-deg>-9</pitch-deg>
        </offsets>
    </model>
    <!-- Accelerometer -->
    <model>
        <name>gmeter2</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/GMeter/GMeter.xml</path>
        <offsets>
            <x-m>-2.185</x-m>
            <y-m>0.21</y-m>
            <z-m>0.748</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>gmeter_screws2</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/GMeter/57mm_screws.xml</path>
        <offsets>
            <x-m>-2.185</x-m>
            <y-m>0.21</y-m>
            <z-m>0.748</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <!-- COM Radio -->
    <model>
        <name>com</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/ar-6201/ar-6201.xml</path>
        <offsets>
            <x-m>-2.19714</x-m>
            <y-m>0</y-m>
            <z-m>0.82</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <model>
        <name>radio_screws</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/GMeter/57mm_screws.xml</path>
        <offsets>
            <x-m>-2.19714</x-m>
            <y-m>0</y-m>
            <z-m>0.82</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <!-- Turn and Slip indicator -->
    <model>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/57mm_turnslip/57mm_turnslip.xml</path>
        <offsets>
            <x-m>-2.182</x-m>
            <y-m>0.065</y-m>
            <z-m>0.725</z-m>
            <pitch-deg>-5.5</pitch-deg>
        </offsets>
    </model>
    <!-- Switches -->
    <model>
        <name>turnslip2_switch</name>
        <path>Aircraft/ASK21/Models/Interior/Panel/Instruments/switches/masterswitch.xml</path>
        <offsets>
            <x-m>-2.185</x-m>
            <y-m>0.115</y-m>
            <z-m>0.725</z-m>
            <pitch-deg>-5</pitch-deg>
        </offsets>
    </model>
    <!--Cockpit Shader-->
    <effect>
        <inherits-from>Aircraft/ASK21/Models/Effects/interior-shadow-back</inherits-from>
        <object-name>panelR1</object-name>
        <object-name>panelR2</object-name>
        <object-name>asi2</object-name>
        <object-name>vario3</object-name>
        <object-name>compass2</object-name>
        <object-name>gmeter2</object-name>
    </effect>
</PropertyList>
