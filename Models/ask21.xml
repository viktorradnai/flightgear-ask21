                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>60</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>60</dep>
            </entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
        <center>
            <x-m>-1.94174</x-m>
            <y-m>0.33063</y-m>
            <z-m>0.77602</z-m>
        </center>
    </animation>
    <!--SHADERS/EFFECTS-->
    <!--Enable/disable windows for better visibility-->
    <animation>
        <type>select</type>
        <condition>
            <property>/sim/model/window</property>
        </condition>
        <object-name>canopy.front.int</object-name>
        <object-name>canopy.back.int</object-name>
        <object-name>sliding_window.front</object-name>
        <object-name>sliding_window.back</object-name>
