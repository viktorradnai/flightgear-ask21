    <!--CANOPY-->
    <animation>
        <type>pick</type>
        <object-name>canopy.frontframe</object-name>
        <visible>true</visible>
        <action>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>doors.canopyF.toggle();</script>
            </binding>
        </action>
    </animation>
    <animation>
        <type>rotate</type>
        <object-name>canopy.frontframe</object-name>
        <object-name>windowF</object-name>
        <property>sim/model/door-positions/canopyF/position-norm</property>
        <interpolation>
            <entry>
                <ind>0.0</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>0.2</ind>
                <dep>0</dep>
            </entry>
            <entry>
                <ind>1</ind>
                <dep>-50</dep>
            </entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
        <center>
            <x-m>-3.56113</x-m>
            <z-m>0.82</z-m>
        </center>
    </animation>
    
    <!--Canopy Front Jettison-->
  
  
    <animation>
        <type>translate</type>
        <object-name>canopy.frontframe</object-name>
        <object-name>windowF</object-name>
        <property>sim/model/door-positions/canopyFJ/position-norm</property>
        <interpolation>
            <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 0.2 </ind><dep> 0.5 </dep></entry>
            <entry><ind> 1.0 </ind><dep> 0.5 </dep></entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
    </animation>
  
    <animation>
        <type>translate</type>
        <object-name>canopy.frontframe</object-name>
        <object-name>windowF</object-name>
        <property>sim/model/door-positions/canopyFJ/position-norm</property>
        <interpolation>
            <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 0.2 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 0.4 </ind><dep> 0.7 </dep></entry>
            <entry><ind> 1.0 </ind><dep> 0.7 </dep></entry>
        </interpolation>
        <axis>
            <y>1</y>
        </axis>
    </animation>
    <animation>
        <type>translate</type>
        <object-name>canopy.frontframe</object-name>
        <object-name>windowF</object-name>
        <property>sim/model/door-positions/canopyFJ/position-norm</property>
        <interpolation>
            <entry><ind> 0.0 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 0.2 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 0.5 </ind><dep> 0.0 </dep></entry>
            <entry><ind> 1.0 </ind><dep> -50 </dep></entry>
        </interpolation>
        <axis>
            <z>1</z>
        </axis>
    </animation>
  <animation>
        <type>select</type>
        <object-name>canopy.frontframe</object-name>
        <object-name>windowF</object-name>
        <condition>
            <less-than>
                <property>sim/model/door-positions/canopyFJ/position-norm</property>
                <value>0.99</value>
            </less-than>
        </condition>
  </animation>
  <!--Back Canopy-->
