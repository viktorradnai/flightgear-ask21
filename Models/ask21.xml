			<less-than>
				<property>sim/model/door-positions/canopyFJ/position-norm</property>
				<value>0.99</value>
			</less-than>
		</condition>
	</animation>
	<!--Back Canopy-->
	<animation>
		<type>pick</type>
		<object-name>canopy.rearframe</object-name>
		<visible>true</visible>
		<action>
			<button>0</button>
			<repeatable>false</repeatable>
			<binding>
				<command>nasal</command>
				<script>doors.canopyB.toggle();</script>
			</binding>
		</action>
	</animation>
	<animation>
		<type>rotate</type>
		<object-name>canopy.rearframe</object-name>
		<object-name>windowR</object-name>
		<property>sim/model/door-positions/canopyB/position-norm</property>
