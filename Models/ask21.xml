<?xml version="1.0"?>
<!-- Schleicher ASK 21 Glider by Heiko Schulz -->
<PropertyList>
	<path>ask21.ac</path>
	<nasal>
		<load>
			var livery_update = aircraft.livery_update.new("Aircraft/ASK21/Models/Liveries", 10);
			
			var rplayer = cmdarg();
			
			rplayer.getNode("sim/model/variant").alias( rplayer.getNode("sim/multiplay/generic/int[14]") );
			var variant = rplayer.getNode("sim/model/variant").getIntValue();
			
			#	Doors
			rplayer.getNode("sim/model/door-positions/canopyF/position-norm" ).alias( rplayer.getNode( "sim/multiplay/generic/float[0]") );
			rplayer.getNode("sim/model/door-positions/canopyFJ/position-norm").alias( rplayer.getNode( "sim/multiplay/generic/float[1]") );
			rplayer.getNode("sim/model/door-positions/canopyB/position-norm" ).alias( rplayer.getNode( "sim/multiplay/generic/float[2]") );
			rplayer.getNode("sim/model/door-positions/frontwin/position-norm").alias( rplayer.getNode( "sim/multiplay/generic/float[3]") );
			rplayer.getNode("sim/model/door-positions/rearwin/position-norm" ).alias( rplayer.getNode( "sim/multiplay/generic/float[4]") );
			
			if( variant == 0 ){
				var livery_update = aircraft.livery_update.new("Aircraft/ASK21/Models/Liveries/glider", 5);
			} else {
				var livery_update = aircraft.livery_update.new("Aircraft/ASK21/Models/Liveries/mi", 5);
			}
		</load>
		<unload>
			livery_update.stop();
			canopyF_door.unalias();
			canopyB_door.unalias();
			
			rplayer.getNode("sim/model/variant").unalias();
			
			#	Doors
			rplayer.getNode("sim/model/door-positions/canopyF/position-norm" ).unalias();
			rplayer.getNode("sim/model/door-positions/canopyFJ/position-norm").unalias();
			rplayer.getNode("sim/model/door-positions/canopyB/position-norm" ).unalias();
			rplayer.getNode("sim/model/door-positions/frontwin/position-norm").unalias();
			rplayer.getNode("sim/model/door-positions/rearwin/position-norm" ).unalias();
		</unload>
	</nasal>
