			<config>
				<target-y-offset-m archive="y" type="double">0.76533</target-y-offset-m>
				<target-z-offset-m archive="y" type="double">-1.0496</target-z-offset-m>
			</config>
		</view>
		<view n="2">
			<config>
				<target-y-offset-m archive="y" type="double">0.76533</target-y-offset-m>
				<target-z-offset-m archive="y" type="double">-1.0496</target-z-offset-m>
			</config>
		</view>
		<view n="3">
			<config>
				<target-y-offset-m archive="y" type="double">0.76533</target-y-offset-m>
				<target-z-offset-m archive="y" type="double">-1.0496</target-z-offset-m>
			</config>
		</view>
		<view n="4">
			<config>
				<target-y-offset-m archive="y" type="double">0.76533</target-y-offset-m>
				<target-z-offset-m archive="y" type="double">-1.0496</target-z-offset-m>
